    "slug": "DollarDash",
    "owner": "dollardash",
    "privacy": "public",
    "description": "This is an expense tracker mobile application",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
